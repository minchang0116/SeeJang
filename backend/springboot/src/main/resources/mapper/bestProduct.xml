<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.springboot.pjt1.repository.mapper.BestProductMapper">
	<select id="selectBestProduct" resultType="bestProduct">
		select * 
		from best_product
		order by ranking;
	</select>

</mapper>